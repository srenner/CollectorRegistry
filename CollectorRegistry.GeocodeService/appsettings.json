{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Kestrel": {
    "EndpointDefaults": {
      "Protocols": "Http2"
    }
  },

  "Geocode": {
    "BaseURL": "https://nominatim.openstreetmap.org/search.php",
    "CityQuery": "city",
    "RegionQuery": "state",
    "PostalQuery": "postalcode",
    "CountryQuery": "country",
    "FormatQuery": "format",
    "Format": "jsonv2",
    "DefaultCountry": "USA"
  }
}
